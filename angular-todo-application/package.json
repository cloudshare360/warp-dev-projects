{
  "name": "angular-todo-application",
  "version": "1.0.0",
  "description": "Full-stack Todo application with Angular frontend and Express.js backend",
  "scripts": {
    "start": "./start-dev.sh",
    "dev": "./start-dev.sh",
    "frontend": "cd Front-End/angular-18-todo-app && npm start",
    "backend": "cd Back-End/express-rest-todo-api && npm run dev",
    "db:start": "cd data-base/mongodb && docker compose up -d",
    "db:stop": "cd data-base/mongodb && docker compose down",
    "db:logs": "cd data-base/mongodb && docker compose logs -f",
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd Back-End/express-rest-todo-api && npm install",
    "install:frontend": "cd Front-End/angular-18-todo-app && npm install",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd Back-End/express-rest-todo-api && npm run build",
    "build:frontend": "cd Front-End/angular-18-todo-app && npm run build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd Back-End/express-rest-todo-api && npm test",
    "test:frontend": "cd Front-End/angular-18-todo-app && npm test",
    "test:api": "./test-api.sh",
    "test:api:full": "cd curl-scripts && ./run-all-tests.sh",
    "clean": "npm run clean:backend && npm run clean:frontend",
    "clean:backend": "cd Back-End/express-rest-todo-api && rm -rf node_modules",
    "clean:frontend": "cd Front-End/angular-18-todo-app && rm -rf node_modules",
    "logs": "tail -f logs/backend.log logs/frontend.log"
  },
  "keywords": ["angular", "express", "mongodb", "todo", "fullstack"],
  "author": "Angular Todo App Developer",
  "license": "MIT",
  "private": true
}